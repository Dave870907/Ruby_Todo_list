

<%= simple_form_for(task) do |f| %>
  <%= f.input :title, label: t('task.title') %>
  
  <%= f.label :start_time, t('task.start_time')%>
  <%= f.datetime_select :start_time%> <br/><br/>

  <%= f.label :end_time, t('task.end_time')%>
  <%= f.datetime_select :end_time%> <br/><br/>

  <%= f.input :status,label: t('task.status') %>
 

  <%= f.input :content,label: t('task.content') %>
  

  <%= f.input :order, label: t("task.order") %>

  <%= f.input :category,label: t('task.category') %>
  
  <%= f.input :user_id, label: t('task.user_id') %>
  

  <%= f.submit %>
<% end %>